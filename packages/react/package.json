{
  "name": "@corbado/react",
  "version": "0.1.0",
  "description": "This package provides all the functionalities and UI components needed by developers when integration Corbado's Authentication management system in their react application",
  "author": "Abdullah Shahbaz <abdullah_ghani@live.com>",
  "homepage": "https://github.com/corbado/javascript#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    ".": "dist"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corbado/javascript.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build:css": "npx tailwindcss -i ./src/styles/index.css -o ./dist/styles.css --minify",
    "copy:assets": "cp -R ./src/assets ./dist",
    "build": "npm run clean && tsc && npm run build:css && npm run copy:assets",
    "publish:local": "npx yalc push --replace  --sig",
    "build_and_publish:local": "npm run build && npm run publish:local",
    "lint": "eslint src --ext .ts,.tsx --fix"
  },
  "bugs": {
    "url": "https://github.com/corbado/javascript/issues"
  },
  "dependencies": {
    "@corbado/react-sdk": "^0.1.3",
    "i18next": "23.5.1",
    "i18next-browser-languagedetector": "7.1.0",
    "react-i18next": "13.2.2"
  },
  "devDependencies": {
    "@types/react": "18.2.23",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "rimraf": "^5.0.5",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": ">=16.8"
  }
}
