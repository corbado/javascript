{
  "name": "@corbado/react",
  "version": "0.1.0",
  "description": "This package provides all the functionalities and UI components needed by developers when integration Corbado's Authentication management system in their react application",
  "author": "Abdullah Shahbaz <abdullah_ghani@live.com>",
  "homepage": "https://github.com/corbado/javascript#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    ".": "dist"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corbado/javascript.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "webpack --config webpack.prod.js",
    "build:local": "npm run clean && tsc",
    "publish:local": "yalc push --replace  --sig",
    "build_and_publish:local": "npm run build:local && npm run publish:local",
    "lint": "eslint src --ext .ts,.tsx --fix"
  },
  "bugs": {
    "url": "https://github.com/corbado/javascript/issues"
  },
  "dependencies": {
    "@corbado/react-sdk": "^0.1.6",
    "@corbado/shared-ui": "^0.1.0",
    "i18next": "23.5.1",
    "i18next-browser-languagedetector": "7.1.0",
    "react-i18next": "13.2.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@corbado/types": "^0.1.0",
    "@corbado/web-core": "^0.1.6",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": ">=16.8"
  }
